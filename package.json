{
  "name": "sveltekit-smui-template",
  "description": "SvelteKit + Svelte Material UI Template",
  "license": "MIT",
  "author": "Paul Pham <git@aquaron.com>",
  "homepage": "https://svetekit-smui-template.aquaron.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:pham/sveltekit-smui-template.git"
  },
  "bugs": {
    "url": "https://github.com/pham/sveltekit-smui-template/issues"
  },
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev --host 0.0.0.0 --port 5002",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "test": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/theme": "^13.0.0",
    "@material/typography": "^13.0.0",
    "@smui/button": "^6.0.0-beta.14",
    "@smui/data-table": "^6.0.0-beta.15",
    "@smui/icon-button": "^6.0.0-beta.14",
    "@smui/textfield": "^6.0.0-beta.14",
    "@sveltejs/adapter-vercel": "^1.0.0-next.44",
    "@sveltejs/app-utils": "next",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.7",
    "smui-theme": "^5.0.0",
    "svelte": "^3.46.4",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@smui/list": "^6.0.0-beta.14",
    "svelte-materialify": "^0.3.11"
  }
}
